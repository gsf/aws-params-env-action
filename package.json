{
  "name": "aws-params-env-action",
  "version": "1.2.4",
  "private": false,
  "description": "GitHub Action to set workflow environment variables from values in AWS Parameter Store. Parameters of type SecureString are masked.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run format-check && npm run lint && npm test && npm run build && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gsf/aws-params-env-action.git"
  },
  "keywords": [
    "actions",
    "aws",
    "parameters"
  ],
  "author": "Sean Fern",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@aws-sdk/client-ssm": "^3.391.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@vercel/ncc": "^0.36.1",
    "aws-sdk-client-mock": "^2.1.1",
    "eslint": "^8.39.0",
    "eslint-plugin-github": "^4.7.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.0.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
